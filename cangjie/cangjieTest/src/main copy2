package cangjieTest


struct Copy {
    var data = 2012
}
class Share {
    var data = 2012
}

let element = '仓颉'
main() {
    let c1 = Copy()
    var c2 = c1
    c2.data = 2023
    println("${c1.data}, ${c2.data}")

    let s1 = Share()
    let s2 = s1
    s2.data = 2023
    println("${s1.data}, ${s2.data}")

    println(element)
    let element = 9
    if (element > 0) {
        let element = 2024
        println(element)
    }
    println(element)
}